{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "elasticloadbalancing:SetRulePriorities",
      "Resource": [
        "arn:aws:elasticloadbalancing:ap-northeast-1:736134917012:listener-rule/net/oke-production/*/*/*",
        "arn:aws:elasticloadbalancing:ap-northeast-1:736134917012:listener-rule/app/oke-production/*/*/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "elasticloadbalancing:DescribeLoadBalancerAttributes",
        "elasticloadbalancing:DescribeLoadBalancers",
        "elasticloadbalancing:DescribeListeners",
        "elasticloadbalancing:DescribeRules"
      ],
      "Resource": "*"
    }
  ]
}
