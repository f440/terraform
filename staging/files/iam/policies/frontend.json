{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": [
                "iam:CreateGroup",
                "iam:DeleteGroup",
                "iam:UpdateGroup"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:AttachGroupPolicy",
                "iam:DeleteGroupPolicy",
                "iam:DetachGroupPolicy",
                "iam:PutGroupPolicy",
                "iam:RemoveUserFromGroup"
            ],
            "Resource": [
                "arn:aws:iam::121659688561:group/frontend"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:AddUserToGroup",
                "iam:AttachGroupPolicy",
                "iam:DeleteGroupPolicy",
                "iam:DetachGroupPolicy",
                "iam:PutGroupPolicy",
                "iam:RemoveUserFromGroup"
            ],
            "Resource": [
                "arn:aws:iam::121659688561:group/qa",
                "arn:aws:iam::121659688561:group/developer",
                "arn:aws:iam::121659688561:group/administrator"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:AttachUserPolicy",
                "iam:DetachUserPolicy",
                "iam:PutUserPolicy"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "NotAction": [
                "sts:AssumeRole"
            ],
            "Resource": "*",
            "Condition" : {
                "BoolIfExists": {
                    "aws:MultiFactorAuthPresent" : true
                }
            }
        }
    ]
}
